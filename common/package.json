{
  "name": "@academia-pro/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types and interfaces for Academia Pro",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
    "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "types",
    "interfaces",
    "academia-pro",
    "school-management"
  ],
  "author": "Academia Pro Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/academia-pro.git"
  },
  "bugs": {
    "url": "https://github.com/academia-pro/issues"
  },
  "homepage": "https://github.com/academia-pro#readme"
}